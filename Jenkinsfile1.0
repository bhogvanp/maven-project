pipeline {
    agent any
tools {
        maven 'locoMaven'
    }	
    stages{
        stage('Build'){
            steps {
                sh 'mvn clean package'
            }
            post {
                success {
                    echo 'Now Archiving...'
                    archiveArtifacts artifacts: '**/target/*.war'
                    echo 'Done Archiving'
                }
            }
        }
    }
}
